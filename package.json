{
  "name": "react-mathtex",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "keywords": [
    "react",
    "react-katex",
    "math",
    "LaTex",
    "TailwindCSS",
    "blog"
  ],
  "license": "MIT",
  "homepage": "https://jorgemath96.github.io",
  "repository": {
    "url": "git+https://github.com/jorgemath96/react-mathtex.git",
    "type": "git"
  },
  "files": [
    "dist/"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "description": "A React component  build using React-Katex and styled with TailwindCSS, to show LaTex content from text written in a simple format. It can be integrated from an form containing an input-text or using for a blog interface.",
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && tsc && npm run copyfiles",
    "clean": "rimraf dist/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "copyfiles": "copyfiles -u 1 src/**/*css dist/"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-katex": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.17",
    "copyfiles": "^2.4.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-katex": "^3.0.1",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^1.2.2"
  },
  "author": "Jorge Osorio",
  "bugs": {
    "url": "https://github.com/jorgemath96/react-mathtex/issues"
  }
}
